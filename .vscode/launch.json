{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "STM32F411 Debug (ST-Link GDB Server)",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "stlink",
      "interface": "swd",

      "cwd": "${workspaceFolder}",
      "executable": "${workspaceFolder}/build/firmware.elf",

      "device": "STM32F411",
      "gdbPath": "arm-none-eabi-gdb",
      "serverpath": "E:/ST/ST-LINK_gdbserver.exe",

      "runToEntryPoint": "main"
    }
  ]
}


